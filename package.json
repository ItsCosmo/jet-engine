{
  "name": "jet-engine",
  "version": "0.1.0",
  "description": "Small footprint template engine",
  "main": "index.js",
  "dependencies": {
    "babel-cli": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "jest": "^21.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "./test-results/coverage",
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "branches": 100,
        "functions": 100,
        "statements": 100
      }
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore ./src/**/*.js ./__tests__/**/*.js ./*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsCosmo/jet-engine.git"
  },
  "keywords": [
    "javascript",
    "HTML",
    "template"
  ],
  "author": "Paul Kleimeyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItsCosmo/jet-engine/issues"
  },
  "homepage": "https://github.com/ItsCosmo/jet-engine#readme"
}
